{{{

  "name": "celora-web",

  "version": 2,  "version": 2,  "$schema": "https://openapi.vercel.sh/vercel.json",

  "buildCommand": "cd apps/web && npm run build",

  "installCommand": "npm install",  "name": "celora-web",  "buildCommand": "cd apps/web && npm install && npm run build",

  "outputDirectory": "apps/web/.next"

}  "builds": [  "outputDirectory": "apps/web/.next",

    {  "installCommand": "echo 'Skipping root install'"

      "src": "apps/web/package.json",}
      "use": "@vercel/next",
      "config": {
        "outputDirectory": "apps/web/.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/web/$1"
    }
  ],
  "functions": {
    "apps/web/src/pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}